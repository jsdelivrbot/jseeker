<script src="https://cdnjs.cloudflare.com/ajax/libs/marked/0.3.6/marked.min.js"></script>
<script>
$(function() {
  marked.setOptions({
    sanitize: true
  });
  $('#preview').html(marked('<%= a.answer %>');
  $('#markdown').keyup(function() {
    var val = $(this).val();
    $('#preview').html(marked(val));
  });
});
</script>
